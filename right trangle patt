#include <stdio.h>

int main()
{
    int row,col,space,n;
    printf("enter no of rows: ");
    scanf("%d",&n);
    
    for(row=1;row<=n;row++)
    {
        for(col=1;col<=row;col++)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}


//output
/*
enter no of rows: 5
*
**
***
****
*****


*/
